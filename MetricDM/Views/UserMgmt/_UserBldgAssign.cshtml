@model MetricDM.Models.BldgAsgnViewModel

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<br />
<div class="row">
    <div class="col-md-4">
        <select name="leftBldgList" id="unasgndBldgList" size="10" style="min-width:200px">
            @foreach (var bldg in Model.unassignedBldgList)
            {
                <option value="@bldg.dsc_mtrc_lc_bldg_id">@bldg.dsc_mtrc_lc_bldg_name</option>
            }
        </select>
    </div>
    <div class="col-md-4">
        <div class="row">
            <button type="button" class="btn" id="btnAsgnBldg">>></button>
        </div>
        <br />
        <div class="row">
            <button type="button" class="btn" id="btnUnasgnBldg"><<</button>
        </div>
    </div>
    <div class="col-md-4">
        <select name="asgndBldgList" id="asgndBldgList" size="10" style="min-width:200px">
            @foreach (var bldg in Model.userBldgList)
            {
                <option value="@bldg.dsc_mtrc_lc_bldg_id">@bldg.dsc_mtrc_lc_bldg_name</option>
            }
        </select>
    </div>
</div>

}